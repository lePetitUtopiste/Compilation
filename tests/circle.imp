var retour;
var espace;

function main(arg) {
  var i;
  i = 0;
  retour = 10;
  espace = 32;

  while (i < arg+1) {
    ligne(i, arg);
    putchar(retour);
    i = i + 1;
  }
}

function ligne(i, n) {
  var j;
  j = 0;
  while (j < n+1) {
    if (i*i + j*j < n*n) {
      putchar(46);
    } else {
      putchar(35);
    }
    putchar(espace);
    j = j+1;
  }
}
